msgstr "<b>[file]</b> faylini yaratib bo‘lmadi. Iltimos, katalog ruxsatlarini tekshiring."

msgctxt "Languages::text_cannot_delete_directory"
msgid "Cannot delete the directory <b>[directory]</b>. Please check the permissions."
msgstr "<b>[directory]</b> katalogini oʻchirib boʻlmadi. Iltimos, ruxsatlarni tekshiring."

msgctxt "Languages::text_cannot_delete_file"
msgid "Cannot delete the file <b>[file]</b>. Please check the permissions."
msgstr "<b>[file]</b> faylini oʻchirib boʻlmadi. Iltimos, ruxsatlarni tekshiring."

msgctxt "Languages::text_cannot_rename_directory"
msgid "Cannot rename the directory <b>[directory]</b>. Please check the permissions."
msgstr "<b>[directory]</b> katalog nomini o‘zgartirib bo‘lmadi. Iltimos, ruxsatlarni tekshiring."

msgctxt "Languages::text_cannot_rename_file"
msgid "Cannot rename the file <b>[file]</b>. Please check the permissions."
msgstr "<b>[file]</b> fayl nomini oʻzgartirib boʻlmadi. Iltimos, ruxsatlarni tekshiring."

msgctxt "Languages::text_cannot_restore_directory"
msgid "Cannot restore the directory <b>[directory]</b> from the repository. Please check if the repository exists."
msgstr "<b>[directory]</b> katalogini ombordan tiklab bo‘lmadi. Iltimos, ombor mavjudligini tekshiring."

msgctxt "Languages::text_cannot_restore_file"
msgid "Cannot restore file <b>[file]</b> from the repository. Please check if the repository exists."
msgstr "<b>[file]</b> faylini ombordan tiklab bo‘lmadi. Iltimos, ombor mavjudligini tekshiring."

msgctxt "Languages::text_can_be_used_once"
msgid "This coupon can be used only once. You have already applied it."
msgstr "Ushbu kupondan faqat bir marta foydalanish mumkin. Siz allaqachon qo'llagansiz."

msgctxt "Languages::text_cart_amount_changed"
msgid "The quantity of the product <b>[product]</b> in your cart  has been changed to allowable."
msgstr "Savatingizdagi mahsulot <b>[product]</b> miqdori ruxsat etilganga o'zgartirildi."

msgctxt "Languages::text_cart_amount_corrected"
msgid "The number of products in the inventory is not enough for your order. The quantity of the product <b>[product]</b> in your cart has been reduced to <b>[count]</b>."
msgstr "Inventarizatsiyadagi mahsulotlar soni buyurtmangiz uchun etarli emas. Savatingizdagi mahsulot <b>[product]</b> miqdori <b>[count]</b> ga kamaytirildi."

msgctxt "Languages::text_cart_empty"
msgid "Your cart is empty"
msgstr "Savatingiz boʻsh"

msgctxt "Languages::text_cart_max_qty"
