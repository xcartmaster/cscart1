msgctxt "Addons::name::tinkoff"
msgid "Tinkoff [Beta]"
msgstr "Тинькофф [Beta]"

msgctxt "Addons::description::tinkoff"
msgid "Allows you to accept payments via Tinkoff"
msgstr "Позволяет принимать платежи через Тинькофф."

msgctxt "Languages::addons.tinkoff.tinkoff_payment"
msgid "Tinkoff"
msgstr "Тинькофф"

msgctxt "Languages::addons.tinkoff.url_for_payment_notifications"
msgid "URL for payment notifications"
msgstr "URL для уведомлений"

msgctxt "Languages::addons.tinkoff.url_for_success"
msgid "URL for successful payments"
msgstr "URL для удавшихся платежей"

msgctxt "Languages::addons.tinkoff.url_for_fail"
msgid "URL for failed payments"
msgstr "URL для неудавшихся платежей"

msgctxt "Languages::addons.tinkoff.terminal_key"
msgid "Terminal"
msgstr "Терминал"

msgctxt "Languages::addons.tinkoff.terminal_key_notice"
msgid "Copy this from your Tinkoff terminal settings"
msgstr "Скопируйте из настроек терминала Тинькофф"

msgctxt "Languages::addons.tinkoff.terminal_password"
msgid "Password"
msgstr "Пароль"

msgctxt "Languages::addons.tinkoff.terminal_password_notice"
msgid "Copy this from your Tinkoff terminal settings"
msgstr "Скопируйте из настроек терминала Тинькофф"

msgctxt "Languages::addons.tinkoff.send_receipt"
msgid "Send receipt"
msgstr "Отправлять чек"

