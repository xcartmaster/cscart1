msgid "Bank accepts the receipt only if the corresponding online cashier option is activated in the terminal settings"
msgstr "Банк принимает чек, только если в настройках терминала активирована соответствующая онлайн-касса."

msgctxt "Languages::addons.tinkoff.pay_type"
msgid "Pay type"
msgstr "Тип оплаты"

msgctxt "Languages::addons.tinkoff.one_step_payment"
msgid "One-step payment"
msgstr "Оплата в один шаг"

msgctxt "Languages::addons.tinkoff.two_step_payment"
msgid "Two-step payment"
msgstr "Оплата в два шага"

msgctxt "Languages::addons.tinkoff.transfer_funds"
msgid "Transfer funds"
msgstr "Перевести средства"

msgctxt "Languages::addons.tinkoff.funds_were_transferred"
msgid "Funds were transferred"
msgstr "Средства переведены"

msgctxt "Languages::addons.tinkoff.payment_status"
msgid "Payment status"
msgstr "Статус платежа"

